{
  "name": "short-link",
  "repository": "https://github.com/Alessandro-Pang/short-link",
  "homepage": "https://short.pangcy.cn/",
  "bugs": "https://github.com/Alessandro-Pang/short-link/issues",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "zi.yang <pcy1314@163.com> (https://github.com/Alessandro-Pang)",
  "scripts": {
    "dev": "concurrently -n \"VUE,FASTIFY\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:web\" \"npm run dev:api\"",
    "dev:api": "cross-env NODE_ENV=development nodemon --exec 'tsx --env-file=.env.development.local' api/index.ts",
    "dev:web": "vite --mode development --host",
    "db:gen-types": "supabase gen types typescript --linked > types/database.types.ts",
    "db:dump": "supabase db dump --linked > config/pg.sql",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@supabase/supabase-js": "^2.89.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.19",
    "fastify": "^5.6.2",
    "pinia": "^3.0.4",
    "pino-pretty": "^13.1.3",
    "qrcode": "^1.5.4",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/qrcode": "^1.5.6",
    "@vitejs/plugin-vue": "^6.0.3",
    "code-inspector-plugin": "^1.3.4",
    "nodemon": "^3.1.11",
    "supabase": "^2.70.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vue-tsc": "^3.2.1"
  }
}
